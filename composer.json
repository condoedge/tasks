{
    "name": "condoedge/tasks",
    "description": "Task managment to your kompo application",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Kompo\\Tasks\\": "src/",
            "Kompo\\TasksSeeders\\": "database/seeders/"
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": "^8.0",
        "laravel/framework": ">=5.6.0",
        "kompo/kompo": "^3.0",
        "kompo/auth": "~0.2"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kompo\\Tasks\\KompoTasksServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
